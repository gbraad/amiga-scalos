versionWB scalos:prefs/Palette.prefsplugin LIKECVER
versionWB "scalos:prefs/Scalos Menu" LIKECVER
versionWB "scalos:prefs/Scalos Palette" LIKECVER
versionWB "scalos:prefs/Scalos Pattern" LIKECVER
versionWB "scalos:prefs/Scalos Prefs" LIKECVER
