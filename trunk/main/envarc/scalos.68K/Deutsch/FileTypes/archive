# Scalos filetype description
# written Sat Jan 22 17:22:48 2005

DESCRIPTION "[Archiv]"

POPUPMENU
	MENUENTRY DEFAULTACTION NAME "Öffnen" UNSELECTEDICON "THEME:Filetypes/Open"
		INTERNALCMD NAME "open"
	ENDMENUENTRY
	MENUENTRY NAME "Informationen anzeigen..." UNSELECTEDICON "THEME:Filetypes/Information"
		INTERNALCMD NAME "iconinfo"
	ENDMENUENTRY
	MENUSEPARATOR
	MENUENTRY NAME "Entpacken nach..."
		CLICMD NAME "xadunfile >NIL: DEST `requestfile drawersonly` %p" STACK "128000" WBARGS
	ENDMENUENTRY
	MENUSEPARATOR
	MENUENTRY NAME "Fixieren"
		INTERNALCMD NAME "snapshot"
	ENDMENUENTRY
	MENUENTRY NAME "Position freigeben"
		INTERNALCMD NAME "unsnapshot"
	ENDMENUENTRY
	MENUSEPARATOR
	SUBMENU NAME "Bearbeiten" UNSELECTEDICON "THEME:Filetypes/Edit"
		MENUENTRY NAME "Ausschneiden" UNSELECTEDICON "THEME:Filetypes/EditCut"
			INTERNALCMD NAME "cut"
		ENDMENUENTRY
		MENUENTRY NAME "Kopieren in Zwischenablage" UNSELECTEDICON "THEME:Filetypes/EditCopy"
			INTERNALCMD NAME "copy"
		ENDMENUENTRY
	ENDSUBMENU
	MENUENTRY NAME "Umbenennen..." UNSELECTEDICON "THEME:Filetypes/Rename"
		INTERNALCMD NAME "rename"
	ENDMENUENTRY
	MENUENTRY NAME "Auslagern"
		INTERNALCMD NAME "leaveout"
	ENDMENUENTRY
	MENUENTRY NAME "Zurücklegen"
		INTERNALCMD NAME "putaway"
	ENDMENUENTRY
	MENUENTRY NAME "Löschen..." UNSELECTEDICON "THEME:Filetypes/Delete"
		INTERNALCMD NAME "delete"
	ENDMENUENTRY
	MENUSEPARATOR
	MENUENTRY NAME "Eigenschaften..." UNSELECTEDICON "THEME:Filetypes/Properties"
		INTERNALCMD NAME "iconproperties"
	ENDMENUENTRY
ENDPOPUPMENU

