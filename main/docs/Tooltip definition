# appicon example
---------------------------------------------------------------------------
TOOLTIP
	GROUP ORIENTATION "horizontal"
		MEMBER HIDE "isempty iconname"
			STRING SRC "iconname" HALIGN "center" STYLE "bold" TEXTPEN "PENIDX_TOOLTIP_TEXT"
		ENDMEMBER
		MEMBER
			STRING SRC "filetypestring" HALIGN "center" TEXTPEN "PENIDX_TOOLTIP_TEXT"
		ENDMEMBER
	ENDGROUP
ENDTOOLTIP
---------------------------------------------------------------------------


# disk icon example
---------------------------------------------------------------------------
TOOLTIP
	GROUP ORIENTATION "vertical"
		MEMBER
			GROUP ORIENTATION "horizontal"
				MEMBER
					STRING SRC "volumeordevicename" STYLE "bold" HALIGN "center" TEXTPEN "PENIDX_TOOLTIP_TEXT"
				ENDMEMBER
				MEMBER
					STRING ID "MSGID_TOOLTIP_DISK" HALIGN "center" TEXTPEN "PENIDX_TOOLTIP_TEXT"
				ENDMEMBER
			ENDGROUP
		ENDMEMBER
		MEMBER
			SPACE SIZE 1
		ENDMEMBER
		MEMBER
			HBAR
		ENDMEMBER
		MEMBER
			SPACE SIZE 1
		ENDMEMBER
		MEMBER
			GROUP ORIENTATION "horizontal"
				MEMBER
					GROUP ORIENTATION "vertical"
						MEMBER HIDE "novolumenode"
							STRING ID "MSGID_TOOLTIP_CREATED" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER HIDE "novolumenode"
							STRING ID "MSGID_TOOLTIP_STATE" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER HIDE "novolumenode"
							STRING ID "MSGID_TOOLTIP_USED" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
					ENDGROUP
				ENDMEMBER
				MEMBER
					GROUP ORIENTATION "vertical"
						MEMBER
							GROUP ORIENTATION "horizontal"
								MEMBER
									STRING SRC "volumecreateddate" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
								MEMBER
									STRING SRC "volumecreatedtime" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
							ENDGROUP
						ENDMEMBER
						MEMBER HIDE "novolumenode"
							STRING SRC "diskstate" HALIGN "left" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER
							GROUP ORIENTATION "horizontal"
								MEMBER
									STRING SRC "diskusagepercent" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
								MEMBER
									STRING TEXT "," HALIGN "center" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
								MEMBER
									STRING SRC "diskusagefree" HALIGN "left" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
							ENDGROUP
						ENDMEMBER
					ENDGROUP
				ENDMEMBER
			ENDGROUP
		ENDMEMBER
	ENDGROUP
ENDTOOLTIP
---------------------------------------------------------------------------


# project icon example
---------------------------------------------------------------------------
TOOLTIP
	GROUP ORIENTATION "vertical"
		INCLUDE "Standard_TTHeader"

		MEMBER HIDE "isempty versionstring" 
			STRING SRC "versionstring" HALIGN "center" TEXTPEN "PENIDX_TOOLTIP_TEXT"
		ENDMEMBER
		MEMBER
			GROUP ORIENTATION "horizontal"
				MEMBER
					GROUP ORIENTATION "vertical"
						MEMBER
							STRING ID "MSGID_TOOLTIP_LASTCHANGE" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER HIDE "isempty filesize"
							STRING ID "MSGID_TOOLTIP_SIZE" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER HIDE "isempty fileprotection"
							STRING ID "MSGID_TOOLTIP_PROTECTION" HALIGN "right" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
					ENDGROUP
				ENDMEMBER
				MEMBER
					GROUP ORIENTATION "vertical"
						MEMBER
							GROUP ORIENTATION "horizontal"
								MEMBER
									STRING SRC "filedate" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
								MEMBER
									STRING SRC "filetime" TEXTPEN "PENIDX_TOOLTIP_TEXT"
								ENDMEMBER
							ENDGROUP
						ENDMEMBER
						MEMBER HIDE "isempty filesize"
							STRING SRC "filesize" HALIGN "left" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
						MEMBER
							STRING SRC "fileprotection" HALIGN "left" TEXTPEN "PENIDX_TOOLTIP_TEXT"
						ENDMEMBER
					ENDGROUP
				ENDMEMBER
			ENDGROUP
		ENDMEMBER
		MEMBER HIDE "isempty filecomment" 
			STRING SRC "filecomment" TEXTPEN "PENIDX_TOOLTIP_TEXT"
		ENDMEMBER
	ENDGROUP
ENDTOOLTIP
---------------------------------------------------------------------------


------------------------------
TOOLTIP
	-
GROUP
	<ORIENTATION=vertical | horizontal>
MEMBER
	<HIDE=(Hide function name)>
STRING
	ID=(name of Message ID from catalog) | TEXT="constant String" | SRC=(stringFunc)
	<HALIGN=LEFT | CENTER | RIGHT> 
	<TEXTPEN=(pen name from prefPensList)> 
	<STYLE=NORMAL | BOLD | ITALIC | BOLDITALIC>
SPACE
	<SIZE=(number of pixels (1..128))>
HBAR
	-
DTIMAGE
	FILENAME=(Datatypes Image file name)
-----------------------------
ISEMPTY (string)
NOVOLUMENODE
-----------------------------
MEMBER
	GROUP | HBAR | STRING | SPACE | DTIMAGE
-----------------------------
STRING SRC
	ICONNAME | VOLUMEORDEVICENAME | FIBFILENAME 
	| VERSIONSTRING | FILETYPESTRING | FILESIZE | FILEDATE 
	| FILETIME | FILEPROTECTION | FILECOMMENT | DISKSTATE 
	| VOLUMECREATEDDATE | VOLUMECREATEDTIME | DISKUSAGE 
	| DISKUSAGEPERCENT | DISKUSAGEFREE | DISKUSAGEINUSE
	| PLUGIN (pluginfilename) | LINKTARGET
-----------------------------
MEMBER HIDE
	NOVOLUMENODE | ISEMPTY (string src function name)
-----------------------------
STRING TEXTPEN pen names
	PENIDX_HSHINEPEN
	PENIDX_HSHADOWPEN
	PENIDX_ICONTEXTOUTLINEPEN
	PENIDX_DRAWERTEXT
	PENIDX_DRAWERTEXTSEL
